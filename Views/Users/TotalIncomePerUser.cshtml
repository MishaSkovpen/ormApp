@model IEnumerable<dynamic>

@{
    ViewData["Title"] = "Total Income Per User";
}

<h2>Total Income Per User</h2>

<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <th>Total Income</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Username</td>
                <td>@item.TotalIncome</td>
            </tr>
        }
    </tbody>
</table>

